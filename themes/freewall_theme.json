{
	"$schema": "vscode://schemas/color-theme",
	"name": "FreeWall's Theme",
	"type": "dark",
	"colors": {
		"editor.background": "#2E2E2E",
		"editor.foreground": "#cee4f1",
		"editor.inactiveSelectionBackground": "#3A3D41",
		"editorIndentGuide.background": "#404040",
		"editor.selectionBackground": "#49483E",
		"editor.findMatchBackground": "#49483e",
		"editor.findMatchBorder": "#CEE4F1",
		"editor.findRangeHighlightBackground": "#1e90ff",
		"editor.findMatchHighlightBackground": "#1e90ff",
		"editorOverviewRuler.warningForeground": "#756D56",
		"minimap.selectionHighlight": "#756D5688",
		"minimap.warningHighlight": "#00000000", //disable
		"minimap.errorHighlight": "#00000000", //disable
		"minimap.findMatchHighlight": "#00000000", //disable
		"minimap.selectionOccurrenceHighlight": "#00000000", //disable
		"editorOverviewRuler.findMatchForeground": "#2ACDFF",
		"editorRuler.foreground": "#3e3e3e",
		"list.dropBackground": "#383B3D",
		"activityBar.background": "#3C3F41",
		"activityBar.foreground": "#ffffff",
		"activityBarBadge.background": "#007ACC",
		"sideBar.background": "#252526",
		"sideBarTitle.foreground": "#BBBBBB",
		"sideBarSectionHeader.background": "#3C3F41",
		"statusBar.background": "#3C3F41",
		"statusBar.foreground": "#cccccc",
		"statusBar.noFolderBackground": "#3C3F41",
		"statusBar.debuggingBackground": "#3C3F41",
		"statusBar.border": "#464646",
		"tab.inactiveBackground": "#3C3F41",
		"tab.border": "#2E2E2E",
		"tab.inactiveForeground": "#d8d8d8",
		"dropdown.background": "#525252",
		"button.background": "#565656",
		"editor.lineHighlightBackground": "#333333",
		"editorCursor.foreground": "#cee4f1",
		"editorWhitespace.foreground": "#505037",
		"editorLineNumber.foreground": "#666666",
		"editorGroupHeader.tabsBackground": "#3C3F41",
		"panelTitle.activeForeground": "#bbbbbb",
		"titleBar.activeBackground": "#3C3F41",
		"titleBar.inactiveBackground": "#3C3F41",
		"titleBar.border": "#2E2E2E",
		"pickerGroup.foreground": "#b0b0b0",
		"terminal.ansiWhite": "#ffffff",
		"gitDecoration.modifiedResourceForeground": "#80CBC4",
		"gitDecoration.untrackedResourceForeground": "#c3e887",
		"gitDecoration.addedResourceForeground": "#c3e887"
	},
	"tokenColors": [
		/*{
			"name": "Function declarations",
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"foreground": "#7af585"
			}
		},*/
		{
			"scope": [
				"support.class",
				"entity.name.function"
			],
			"settings": {
				"foreground": "#cee4f1"
			}
		},
		{
			"scope":[
				"entity.name.type",
				"entity.name.class",
				"storage.type.primitive.java",
				"storage.type.primitive.array.java",
				"storage.type.object.array.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.token.java",
				"support.class.component.tsx"
			],
			"settings": {
				"foreground": "#4EC9B0"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"meta.return-type",
				"support.type",
				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",
				"storage.type.annotation.java",
				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy"
			],
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#fda6ff"
			}
		},
		{
			"name": "Control flow keywords",
			"scope": "keyword.control",
			"settings": {
				"foreground": "#C586C0"
			}
		},
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable",
				"support.other.variable",
			],
			"settings": {
				"foreground": "#ffc800"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#ffc800"
			}
		},
		{
			"name": "CSS property value",
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#66D9EF"
			}
		},
		{
			"settings": {
				"foreground": "#cee4f1",
				"background": "#1E1E1E"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#000080"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#608b4e"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#fda6ff"
			}
		},
		{
			"scope": "constant.regexp",
			"settings": {
				"foreground": "#646695"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#f25f6a"
			}
		},
		{
			"scope": "entity.name.tag.css",
			"settings": {
				"foreground": "#f25f6a"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.class.mixin.css",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.pseudo-element.css",
				"source.css.less entity.other.attribute-name.id",
				"entity.other.attribute-name.attribute.scss",
				"entity.other.attribute-name.scss"
			],
			"settings": {
				"foreground": "#A6E22E"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e3c980"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e3c980"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#7ccff1"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"scope": "beginning.punctuation.definition.quote.markdown",
			"settings": {
				"foreground": "#608b4e"
			}
		},
		{
			"scope": "beginning.punctuation.definition.list.markdown",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#7ccff1"
			}
		},
		{
			"scope": "meta.selector",
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#7ccff1"
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#ffc800"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"scope": "storage.modifier",
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#7ccff1"
			}
		},
		{
			"scope": "string.tag",
			"settings": {
				"foreground": "#7ccff1"
			}
		},
		{
			"scope": "string.value",
			"settings": {
				"foreground": "#7ccff1"
			}
		},
		{
			"name": "JavaScript string interpolation ${}",
			"scope": [
				"punctuation.definition.template-expression.begin.js",
				"punctuation.definition.template-expression.begin.ts",
				"punctuation.definition.template-expression.end.ts",
				"punctuation.definition.template-expression.end.js"
			],
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"variable.css",
				"variable.scss",
				"variable.other.less"
			],
			"settings": {
				"foreground": "#ffc800"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.logical.python"
			],
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"foreground": "#cee4f1"
			}
		},
		{
			"scope": "support.function.git-rebase",
			"settings": {
				"foreground": "#ffc800"
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#e3c980"
			}
		},
		{
			"name": "coloring of the Java import and package identifiers",
			"scope": [
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#cee4f1"
			}
		},
		{
			"name": "this.self",
			"scope": "variable.language",
			"settings": {
				"foreground": "#ffc800"
			}
		}
	]
}
